<odoo>
  <data>
      <report id="etiquetas_report" string="Etiquetas" model="stock.picking" report_type="qweb-pdf" name="etiquetas.etiquetas_custom_template"/>
      <template id="etiquetas_custom_template">
              <t t-call="web.html_container">
        <div class="page">
            <head>
                <style>table 
                          {border-collapse: collapse;}
                          table, td, th {border: 1px solid black;}
                          </style>
            </head>
          <span t-esc="concentrado(docs)"/>
            <t t-foreach="docs" t-as="pick">
                <table  class="table table-sm" t-if="pick.state=='done' or pick.state=='assigned' and pick.value2 == 1" style="width:100%;border-collapse: collapse;">
                    <tbody>
                        <t style="width:100%" t-foreach="pick.move_ids_without_package" t-as="move">
                                <td style="width:20%">
                                    <span t-field="pick.product_id.name"/>
                                    <span> | </span>
                                    <span t-field="pick.sale_id.name"/>
                                    <span> | </span>
                                    <span>Local</span>
                                </td>
                        </t>
                    </tbody>
                </table>
            </t>
        </div>
      </template>
  </data>
</odoo>
